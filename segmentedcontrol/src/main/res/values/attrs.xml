<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SegmentedControl">
        <attr name="maxPills" format="integer"/>
        <attr name="segmentSelectorStartBackground" format="reference"/>
        <attr name="segmentStartFillBackground" format="reference"/>
        <attr name="segmentSelectorCenterBackground" format="reference"/>
        <attr name="segmentCenterFillBackground" format="reference"/>
        <attr name="segmentSelectorEndBackground" format="reference"/>
        <attr name="segmentEndFillBackground" format="reference"/>
        <attr name="segmentSelectedBackground" format="reference"/>
        <attr name="segmentTextColor" format="color|reference"/>
        <attr name="segmentSelectedTextColor" format="color|reference"/>
        <attr name="showCloseIcon" format="boolean"/>
        <attr name="segmentCloseIcon" format="reference"/>
        <attr name="segmentMarginTop" format="dimension"/>
        <attr name="segmentMarginBottom" format="dimension"/>
        <attr name="segmentMarginLeft" format="dimension"/>
        <attr name="segmentMarginRight" format="dimension"/>
        <attr name="segmentPaddingTop" format="dimension"/>
        <attr name="segmentPaddingBottom" format="dimension"/>
        <attr name="segmentPaddingLeft" format="dimension"/>
        <attr name="segmentPaddingRight" format="dimension"/>
        <attr name="segmentCloseIconMarginLeft" format="dimension"/>
        <attr name="segmentSelectionMode">
            <enum name="none" value="0"/>
            <enum name="multi" value="1"/>
        </attr>
    </declare-styleable>
</resources>